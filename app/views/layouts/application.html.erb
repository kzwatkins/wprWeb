<!DOCTYPE html>
<html>
<head>
  <title><%= yield (:title) %> | WPR </title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render 'shared/navbar' %>
	<div class="main-page-content <%= controller.controller_name %>">
		<div class="main-logo">
			<img src="/assets/wprfullLogo.png" alt="WPR Logo" width="50%">
		</div>
		
		<%= yield %>
	</div>

</body>
</html>
